{
  "root": true,
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "ecmaVersion": 2021,
    "sourceType": "module",
    "extraFileExtensions": [".vue"]
  },
  "plugins": [
    "@typescript-eslint",
    "vue"
  ],
  "extends": [
    "plugin:vue/vue3-recommended"
  ],
  "rules": {
    "vue/multi-word-component-names": "off"
  }
}
